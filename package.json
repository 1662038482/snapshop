{
    "name": "snapshop",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --host",
        "build": "vue-tsc --noEmit && vite build",
        "build:vscode": "vue-tsc --noEmit && vite build --config vite.config.vscode.ts --mode vscode-production",
        "watch:vscode": "vite build --config vite.config.vscode.ts --mode vscode-development",
        "preview": "vue-tsc --noEmit && vite build && vite preview"
    },
    "devDependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "@types/node": "^16.0.0",
        "@vitejs/plugin-vue": "^4.2.1",
        "@vueuse/core": "^7.7.1",
        "ant-design-vue": "^3.2.20",
        "axios": "^1.4.0",
        "jimp": "0.16.1",
        "less": "^4.1.3",
        "pinia": "^2.0.35",
        "typescript": "^5.0.0",
        "unplugin-vue-components": "^0.24.1",
        "vite": "^4.3.4",
        "vite-plugin-singlefile": "^0.13.5",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6",
        "vue-tsc": "^1.6.3"
    }
}
