{
  "name": "snapshop",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "build:vscode": "vue-tsc --noEmit && vite build --config vite.config.vscode.ts --mode vscode-production",
    "watch:vscode": "vite build --config vite.config.vscode.ts --mode vscode-development",
    "preview": "vue-tsc --noEmit && vite build && vite preview"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vueuse/core": "^7.7.1",
    "ant-design-vue": "^3.2.6",
    "axios": "^0.25.0",
    "jimp": "^0.16.1",
    "pinia": "^2.0.9",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@vitejs/plugin-vue": "^2.0.0",
    "less": "^4.1.2",
    "typescript": "^4.4.4",
    "unplugin-vue-components": "^0.17.11",
    "vite": "^2.7.2",
    "vite-plugin-singlefile": "^0.6.3",
    "vue-tsc": "^0.29.8"
  }
}
